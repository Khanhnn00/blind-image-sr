{
    "mode": "sr",
    "use_cl": false,
    "gpu_ids": [1],

    "scale": 4,
    "is_train": false,
    "use_chop": true,
    "rgb_range": 255,
    "self_ensemble": false,
    "save_image": true,

    "datasets": {
        "val_SR": {
            "mode": "LRHR",
            "dataroot_HR": "../SRbenchmark/BSD100",
            "data_type": "img",
            "phase": "test",
            "scale": 4,
            "kernel_size": 19,
            "rgb_range": 255
        },
        "val_kernel": {
            "mode": "LRHR_kernel",
            "dataroot_HR": "../SRbenchmark/BSD100",
            "data_type": "img",
            "phase": "test",
            "scale": 4,
            "kernel_size": 19,
            "rgb_range": 255
        }
    },

    "network": {
        "which_model": "STUPIDCATE",
        "SR":{
            "num_features": 64,
            "in_channels": 3,
            "out_channels": 3,
            "num_blocks": 16,
            "res_scale": 0.1,
            "upscale_factor": 4
        },
        "KernelExtractor":{
            "in_channels": 3,
            "out_channels": 1,
            "nf": 64,
            "kernel_dim": 316,
            "norm": "none",
            "use_sharp": false,
            "n_blocks": 4,
            "padding_type": "reflect",
            "use_dropout": false,
            "front_rbs": 10,
            "kernel_size": 19
        },
        "DIP":{
            "input_nc": 8,
            "output_nc": 3,
            "nf": 64,
            "norm": "none",
            "padding_type": "reflect"
        } 
    },

    "num_iters": 5000,
    "reg_noise_std": 0.0001,
    "x_lr": 5e-4,
    "num_warmup_iters": 300,

    "solver": {
        "pretrain_SR": null,
        "pretrain_netG": null,
        "netG_only": false,
        "pretrainedSR_path": "./experiments/STUPIDCATE_in3f64_x4/epochs/SR_last_ckp.pth",
        "pretrainednetG_path": "./experiments/STUPIDCATE_in3f64_x4/epochs/netG_best_ckp.pth"
    }
}